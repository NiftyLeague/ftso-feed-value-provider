{
  "name": "ftso-feed-value-provider",
  "version": "1.0.0",
  "description": "Retrieves data from exchanges and supplies current feed values (prices)",
  "author": "0xPlayerOne",
  "private": true,
  "license": "GPL-3.0-only",
  "scripts": {
    "_comment_build": "================== BUILD & START =================",
    "build": "nest build",
    "start": "NODE_OPTIONS=--max-old-space-size=1024 nest start",
    "start:dev": "NODE_OPTIONS=--max-old-space-size=1024 nest start --watch",
    "start:debug": "LOG_LEVEL=debug NODE_OPTIONS=--max-old-space-size=1024 nest start --debug --watch",
    "start:prod": "node --max-old-space-size=1024 dist/src/main",
    "_comment_quality": "================= CODE QUALITY =================",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"**/*.{js,ts,md,mdx,json}\"",
    "format:check": "prettier --check \"**/*.{js,ts,md,mdx,json}\"",
    "type:check": "tsc --noEmit",
    "validate": "pnpm format:check && pnpm lint:check && pnpm type:check",
    "_comment_jest": "==================== JEST TESTS ===================",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:jest": "./scripts/run.sh test jest",
    "test:accuracy": "./scripts/run.sh test accuracy",
    "test:endurance": "./scripts/run.sh test endurance",
    "test:integration": "./scripts/run.sh test integration",
    "test:performance": "./scripts/run.sh test performance",
    "test:unit": "./scripts/run.sh test unit",
    "_comment_tests": "================== SYSTEM TESTS ==================",
    "test:all": "./scripts/run.sh test all",
    "test:validate": "./scripts/run.sh test validate",
    "test:readiness": "./scripts/run.sh test readiness",
    "test:server": "./scripts/run.sh test server",
    "test:security": "./scripts/run.sh test security",
    "test:load": "./scripts/run.sh test load",
    "test:shutdown": "./scripts/run.sh test shutdown",
    "test:feeds": "./scripts/run.sh test feeds",
    "test:docker": "./scripts/run.sh test docker",
    "_comment_debug": "=============== DEBUG & MONITORING ===============",
    "debug:all": "./scripts/run.sh debug all",
    "debug:startup": "./scripts/run.sh debug startup",
    "debug:websockets": "./scripts/run.sh debug websockets",
    "debug:performance": "./scripts/run.sh debug performance",
    "debug:feeds": "./scripts/run.sh debug feeds",
    "debug:errors": "./scripts/run.sh debug errors",
    "debug:cache": "./scripts/run.sh debug cache",
    "debug:config": "./scripts/run.sh debug config",
    "debug:integration": "./scripts/run.sh debug integration",
    "debug:resilience": "./scripts/run.sh debug resilience",
    "debug:data-aggregation": "./scripts/run.sh debug data-aggregation",
    "_comment_audit": "================== SYSTEM AUDIT ==================",
    "audit:summary": "./scripts/run.sh utils audit summary",
    "audit:full": "./scripts/run.sh utils audit full",
    "audit:analyze": "./scripts/run.sh utils audit analyze",
    "audit:baseline": "./scripts/run.sh utils audit baseline",
    "audit:status": "./scripts/run.sh utils audit status",
    "audit:clean": "./scripts/run.sh utils audit clean",
    "_comment_docker": "==================== DOCKER =====================",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart ftso-provider",
    "docker:logs": "docker-compose logs -f ftso-provider",
    "docker:logs:tail": "docker-compose logs --tail=100 ftso-provider",
    "docker:ps": "docker-compose ps",
    "docker:test": "./scripts/run.sh test docker",
    "docker:rebuild": "docker-compose down && docker-compose build --no-cache && docker-compose up -d",
    "docker:shell": "docker-compose exec ftso-provider sh",
    "_comment_tickers": "================== DEV TOOLS ===================",
    "tickers:generate": "./scripts/run.sh utils tickers",
    "tickers:fetch": "./scripts/run.sh utils tickers --fetch-only",
    "tickers:feeds": "./scripts/run.sh utils tickers --feeds-only",
    "tickers:help": "./scripts/run.sh utils tickers --help",
    "_comment_git_hooks": "================== GIT HOOKS ===================",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "axios": "^1.13.2",
    "ccxt": "^4.5.18",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "helmet": "^8.1.0",
    "nodemailer": "^7.0.10",
    "protobufjs": "^7.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.3",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "swagger-typescript-api": "^13.2.16",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22"
  },
  "engineStrict": true,
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
